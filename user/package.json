{
  "name": "@pagopa/guild-grpc-user",
  "description": "User http and gRpc implementation",
  "author": "Pietro Tota",
  "repository": "https://github.com/pagopa/guild-grpc",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build:tsc": "tsc",
    "build": "npm-run-all -s clean build:* ",
    "clean": "npm-run-all -s clean:*",
    "clean:dist": "rimraf dist",
    "run-tests-with-docker": "npm-run-all -s clean webpack run-test-with-docker:*",
    "run-test-with-docker:get-near-vehicle": "docker run --env-file .env -i --rm -v $(pwd)/dist:/dist grafana/k6 run /dist/near-position-test.js",
    "run-tests-locally": "npm-run-all -s clean webpack run-test-locally:*",
    "run-test-locally:get-near-vehicle": "export $(cat .env | xargs) && k6 run $(pwd)/dist/near-position-test.js",
    "webpack": "export SET NODE_OPTIONS=--openssl-legacy-provider && shx rm -rf dist && webpack && cp -r definitions dist/definitions"
  },
  "devDependencies": {
      "@babel/core": "7.13.16",
      "@babel/plugin-proposal-class-properties": "7.13.0",
      "@babel/plugin-proposal-object-rest-spread": "7.13.8",
      "@babel/preset-env": "7.13.15",
      "@babel/preset-typescript": "7.13.0",
      "@types/webpack": "5.28.0",
      "@pagopa/danger-custom-rules": "^2.0.3",
      "@pagopa/eslint-config": "^1.3.1",
      "@pagopa/openapi-codegen-ts": "^11.1.0",
      "@types/k6": "^0.37.1",
      "babel-loader": "8.2.2",
      "clean-webpack-plugin": "4.0.0-alpha.0",
      "copy-webpack-plugin": "^9.0.1",
      "webpack": "5.35.1",
      "webpack-cli": "4.6.0",
      "webpack-glob-entries": "^1.0.1",
      "danger": "^10.3.0",
      "dependency-check": "^4.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "modclean": "^3.0.0-beta.1",
      "npm-run-all": "^4.1.5",
      "oval": "^1.0.0",
      "prettier": "^1.18.2",
      "shx": "^0.3.2",
      "typescript": "^4.3.5"
  },
  "dependencies": {
      "@pagopa/ts-commons": "^10.7.1",
      "k6": "^0.0.0",
      "k6-stat": "1.0.6",
      "fp-ts": "^2.10.5",
      "io-ts": "^2.2.16",
      "lodash": "^4.17.21",
      "node-fetch": "^2.6.7"
  },
  "resolutions": {
    "handlebars": "~4.7.7",
    "minimist": "~1.2.6"
  }
}
